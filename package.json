{
  "name": "livehub",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean build:prod",
    "build:dev": "webpack --progress --color",
    "build:prod": "webpack -p --progress --color --env.prod 1",
    "clean": "rimraf public/manifest.json public/assets/**/*.*",
    "dev": "npm-run-all clean \"build:dev -- --watch\""
  },
  "dependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-modern-node": "^2.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "foundation-sites": "^6.2.1",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.3",
    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "node-sass": "^3.7.0",
    "npm-run-all": "^2.1.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "url-loader": "^0.5.7",
    "vue": "^1.0.24",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.3.1",
    "vue-style-loader": "^1.0.0",
    "webpack": "^2.1.0-beta.7",
    "webpack-manifest-plugin": "danethurber/webpack-manifest-plugin#unamed-chunks"
  },
  "devDependencies": {}
}
