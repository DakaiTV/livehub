{
	"name": "t2t2/livehub",
	"description": "I'm alive.",
	"keywords": [],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"barryvdh/laravel-ide-helper": "~2.0",
		"illuminate/html": "~5.0",
		"league/fractal": "0.11.*",
		"guzzlehttp/guzzle": "~5.0",
		"react/promise": "~2.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"doctrine/dbal": "~2.5"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"files": [
			"app/helpers.php"
		],
		"psr-4": {
			"t2t2\\LiveHub\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
